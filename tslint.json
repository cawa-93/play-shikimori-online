{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "no-console": [
      true,
      "log",
      "table"
    ],
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-empty": [
      true,
      "allow-empty-catch"
    ]
  }
}
